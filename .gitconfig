[core]
	excludesfile = /opt/boxen/config/git/gitignore
	editor = subl -w
[alias]
	st = status
	di = diff
	co = checkout
	ci = commit
	br = branch
	sta = stash
	llog = log --date=local
[apply]
	whitespace = nowarn
[color]
	branch = auto
	diff = auto
	interactive = auto
	status = auto
	ui = auto
[branch]
	autosetupmerge = true
[user]
	name = Fred Oliveira
	email = fred@helloform.com
[push]
	default = simple

[credential]
helper = /opt/boxen/bin/boxen-git-credential

[hub]
protocol = https
